{
  "name": "warrendeleon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode Release",
    "android:apk": "cd android && ./gradlew assembleRelease && cd .. && echo '\\nâœ… APK built at: android/app/build/outputs/apk/release/app-release.apk'",
    "android:aab": "cd android && ./gradlew bundleRelease && cd .. && echo '\\nâœ… AAB built at: android/app/build/outputs/bundle/release/app-release.aab'",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --scheme warrendeleon-Prod --mode Release",
    "ios:pods": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "yarn clean:android && yarn clean:ios && yarn clean:node",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods && cd .. && yarn ios:pods",
    "clean:node": "rm -rf node_modules && yarn install",
    "validate": "yarn typecheck && yarn lint && yarn test",
    "prepare": "husky",
    "doctor": "react-native doctor",
    "detox:ios:build": "detox build -c ios.sim.debug",
    "detox:ios:test": "DETOX_LOGLEVEL=error DETOX_CONFIGURATION=ios.sim.debug TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js --format ./src/test-utils/cucumber/formatters/CheckmarkFormatter.js --require-module ts-node/register --require-module tsconfig-paths/register 'src/features/**/__tests__/*.feature' --require 'src/test-utils/cucumber/**/*.{ts,tsx}' --require 'src/features/**/__tests__/*.cucumber.{ts,tsx}'",
    "detox:android:build": "detox build -c android.emu.debug",
    "detox:android:test": "DETOX_LOGLEVEL=error DETOX_CONFIGURATION=android.emu.debug TS_NODE_PROJECT=tsconfig.cucumber.json cucumber-js --format ./src/test-utils/cucumber/formatters/CheckmarkFormatter.js --require-module ts-node/register --require-module tsconfig-paths/register 'src/features/**/__tests__/*.feature' --require 'src/test-utils/cucumber/**/*.{ts,tsx}' --require 'src/features/**/__tests__/*.cucumber.{ts,tsx}'",
    "e2e:ios": "yarn detox:ios:build && yarn detox:ios:test",
    "e2e:android": "yarn detox:android:build && yarn detox:android:test"
  },
  "dependencies": {
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/themed": "^1.1.73",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.13.2",
    "i18next": "^25.6.0",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.553.0",
    "nativewind": "^4.2.1",
    "react": "19.1.1",
    "react-dom": "18.3.1",
    "react-i18next": "^16.2.4",
    "react-native": "0.82.1",
    "react-native-blob-util": "^0.23.2",
    "react-native-bootsplash": "^6.3.11",
    "react-native-config": "1.5.6",
    "react-native-localize": "^3.6.0",
    "react-native-pdf": "^7.0.3",
    "react-native-reanimated": "4.0.2",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-share": "^12.2.1",
    "react-native-svg": "^15.14.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "0.4.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/runtime": "^7.25.0",
    "@cucumber/cucumber": "^12.2.0",
    "@eslint/js": "^9.39.1",
    "@react-native-community/cli": "^20.0.2",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.45.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-test-renderer": "19.1.1",
    "reactotron-react-native": "^5.1.18",
    "reactotron-redux": "^3.2.1",
    "tailwindcss": "3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.6.4",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
